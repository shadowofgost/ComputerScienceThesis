	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Flask",
			"type": "python",
			"request": "launch",
			"module": "flask",
			"env": { "FLASK_APP": "./Code/xuemc_win/run.py", "FLASK_ENV": "development" },
			"args": ["run", "--no-debugger"],
			"jinja": true,
			"justMyCode": true
		},
		{
			"name": "Python",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "FastApi",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}\\Code\\src\\main.py",
			"console": "integratedTerminal"
		},
		{
			"type": "python",
			"request": "launch",
			"module": "uvicorn",
			"args": ["main:app"],
			"jinja": true,
			"justMyCode": true
		}
	]
}
